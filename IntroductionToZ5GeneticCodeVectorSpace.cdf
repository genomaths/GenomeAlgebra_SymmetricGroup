Cell[TextData[{
 "Next, we can define de inner product of two codons ",
 Cell[BoxData[
  FormBox[
   RowBox[{"x", "\[Element]", 
    SuperscriptBox["\[ScriptCapitalB]", "3"]}], TraditionalForm]],
  ExpressionUUID->"19ebb570-3a9a-4886-bdd2-487cc8ebd5a4"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"y", "\[Element]", 
    SuperscriptBox["\[ScriptCapitalB]", "3"]}], TraditionalForm]],
  ExpressionUUID->"9fef8165-96cd-4386-9b37-ec0545b692c4"],
 " by means of the bijection ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["acgu",
     FontSlant->"Italic"], "(", 
    StyleBox["x",
     FontSlant->"Italic"], ")"}], TraditionalForm]],ExpressionUUID->
  "f0cddfde-d265-4d83-a130-fcfe38bd4cb2"],
 " as follow: \[LeftAngleBracket]",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["ACGU",
     FontSlant->"Italic"], "(", 
    StyleBox["x",
     FontSlant->"Italic"], ")"}], TraditionalForm]],ExpressionUUID->
  "30d3345d-7141-4864-b26f-1c0744544add"],
 ",",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["ACGU",
     FontSlant->"Italic"], "(", 
    StyleBox["y",
     FontSlant->"Italic"], ")"}], TraditionalForm]],ExpressionUUID->
  "5f465e52-765c-40c7-b9e9-e92bdae3a447"],
 "\[RightAngleBracket]",
 Cell[BoxData[
  FormBox[
   RowBox[{"=", 
    RowBox[{
     RowBox[{
      SubscriptBox["x", "1"], 
      SubscriptBox["y", "1"]}], "+", 
     RowBox[{
      SubscriptBox["x", "2"], 
      SubscriptBox["y", "2"]}], "+", 
     RowBox[{
      SubscriptBox["x", "3"], 
      SubscriptBox["y", "3"]}]}]}], TraditionalForm]],ExpressionUUID->
  "d2c061fb-cbf2-42df-a852-20fabe3c85a3"],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["x", "i"], ",", 
     RowBox[{
      SubscriptBox["y", "i"], "\[Element]", 
      SubsuperscriptBox["\[DoubleStruckCapitalZ]", "5", "3"], "\[Subset]", 
      SuperscriptBox["\[DoubleStruckCapitalR]", "3"]}]}], ")"}], 
   TraditionalForm]],ExpressionUUID->"2330ca9e-6aaf-4ba1-a0d0-3626179c8aa7"],
 " and \[LeftAngleBracket]",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["acgu",
     FontSlant->"Italic"], "(", 
    StyleBox["x",
     FontSlant->"Italic"], ")"}], TraditionalForm]],ExpressionUUID->
  "dfa25747-7fda-492a-a5e6-01d2f5134943"],
 ",",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["acgu",
     FontSlant->"Italic"], "(", 
